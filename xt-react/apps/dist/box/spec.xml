      <entry key="[IB,woodBox,dvdCase]" width="0" height="0"/>
    </parameter>

    <!--  天窗出血  -->
    <parameter id="cameoBleed" keyPattern="cameo">
      <entry key="none" top="0" bottom="0" left="0" right="0" />
      <entry key="Box_M" top="60" bottom="60" left="60" right="60" />
    </parameter>
    <!--  天窗尺寸  -->
    <parameter id="cameoSize" keyPattern="cameoShape">
      <entry key="none" width="0" height="0" />
      <entry key="Box_Rect" width="885" height="885" />
      <entry key="Box_Round" width="885" height="885" />
    </parameter>

