<?xml version="1.0" encoding="UTF-8"?>
<product-spec version="4.0">
  <global>
    <dpi>300</dpi>
    <imageQualityBufferPercent>30</imageQualityBufferPercent>
    <imagedimenstions>
      <imagedimenstion viewWidth="96" viewHeight="96">96x96</imagedimenstion>
      <imagedimenstion viewWidth="350" viewHeight="350">350x350</imagedimenstion>
      <imagedimenstion viewWidth="700" viewHeight="700">700x700</imagedimenstion>
      <imagedimenstion viewWidth="1000" viewHeight="1000">1000x1000</imagedimenstion>
      <imagedimenstion viewWidth="1500" viewHeight="1500">1500x1500</imagedimenstion>
    </imagedimenstions>
    <options>
      <!-- imagebox name -->
      <optionGroup id="product">
        <!--以前的id为IB需要在解析的时候做兼容-->
        <option id="IB">
          <title>Image Box</title>
        </option>
        <option id="woodBox">
          <title>Wood Box</title>
        </option>
        <option id="usbCase">
          <title>USB Case</title>
        </option>
        <option id="dvdCase">
          <title>DVD Case</title>
        </option>
      </optionGroup>
      <!-- size -->
      <optionGroup id="size">
        <option id="6.5X6.5" name="6.5X6.5"/>
        <option id="4X6" name="4X6"/>
        <option id="5X7" name="5X7"/>
        <option id="8X10" name="8X10"/>
        <option id="8X8" name="8X8"/>
        <option id="8X11" name="8X11"/>
        <option id="11X8" name="11X8"/>
        <option id="10X10" name="10X10"/>
        <option id="11X14" name="11X14"/>
        <option id="14X11" name="14X11"/>
        <option id="12X12" name="12X12"/>
        <option id="12X16" name="12X16"/>
        <option id="16X12" name="16X12"/>
        <option id="14X14" name="14X14"/>
      </optionGroup>
      <!-- type -->
      <optionGroup id="type">
        <option id="none" name="None" />
        <option id="IW" name="Image Wrapped"/>
        <option id="BL" name="Black Leatherette"/>
        <option id="IP" name="Image Panel"/>
        <option id="BP" name="Black Panel"/>
      </optionGroup>
      <!-- cover-->
      <optionGroup id="cover">
        <option id="none" name="None" />
        <option id="HC" name="Hard Cover" />
        <option id="NC" name="Linen Cover" />
        <option id="LC" name="Leatherette" />
        <option id="BC" name="Bling Cover" />
      </optionGroup>
      <!-- leatherColor -->
      <optionGroup id="leatherColor">
        <option id="none" name="None" />
        <option id="black" name="Black" />
        <option id="red" name="Ruby" />
        <option id="white" name="White" />
        <option id="espresso" name="Espresso" />
        <option id="camel" name="Cinnamon" />
        <option id="blue" name="Blue" />
        <option id="kiwi" name="Kiwi" />
        <option id="pink" name="Pink" />
        <option id="navyBlue" name="Navy Blue" />
        <option id="shamrockGreen" name="Shamrock Green" />
        <option id="tangerineOrange" name="Tangerine Orange" />
        <option id="slateGrey" name="Slate Grey" />
        <option id="maroon" name="Maroon" />
        <option id="lightBlue" name="Light Blue" />
        <option id="siennaBrown" name="Sienna Brown" />
        <option id="cream" name="Cream" />
        <option id="oliveGreen" name="Olive Green" />
        <option id="indianRed" name="Indian Red" />
        <option id="chocolateBrown" name="Chocolate Brown" />
        <option id="brown" name="Dark Brown" />
        <option id="darkGreen" name="Dark Green" />
        <option id="orange" name="Brown" />
        <option id="tan" name="Tan" />
        <option id="rusticGreen" name="Rustic Green" />
        <option id="rusticBrown" name="Rustic Brown" />
        <option id="blingLace" name="Lace" />
        <option id="blingZebra" name="Zebra" />
        <option id="blingCream" name="Cream" />
        <option id="blingBlack" name="Black Grid" />
        <option id="blingBlue" name="Blue Grid" />
        <option id="blingEspresso" name="Espresso Grid" />
        <option id="blingRed" name="Red Grid" />
        <option id="blingWhite" name="White Grid" />
        <option id="greenLeaf" name="Green Leaf" />
        <option id="fireBrick" name="Fire Brick" />
        <option id="pinkRose" name="Pink Rose" />
        <option id="pansy" name="Pansy" />
        <option id="blueGrey" name="Blue Grey" />
        <option id="sunnyYellow" name="Sunny Yellow" />
        <option id="cinnamon" name="Cinnamon" />
        <option id="ruby" name="Ruby" />
        <option id="blackNavyBlue" name="Navy Blue" />
      </optionGroup>
      <!-- spine -->
      <optionGroup id="spineThickness">
        <option id="none" name="None"/>
        <option id="T" name="Thick"/>
        <option id="L" name="Thin"/>
        <option id="N" name="Normal"/>
      </optionGroup>
      <optionGroup id="finish">
        <option id="none">
            <title>None</title>
        </option>
        <option id="matte">
            <title>Matte</title>
        </option>
        <option id="glossy">
            <title>Glossy</title>
        </option>
      </optionGroup>
      <optionGroup id="dvdType">
        <option id="none" default="true">
            <title>None</title>
        </option>
        <option id="one">
            <title>One DVD Holder</title>
        </option>
        <option id="two">
            <title>Two DVD Holders</title>
        </option>
      </optionGroup>
      <optionGroup id="dvdPrinted">
        <option id="none" default="true">
            <title>None</title>
        </option>
        <option id="noPrinted">
            <title>No Printed</title>
        </option>
        <option id="customerPrinted">
            <title>Customer Printed</title>
        </option>
      </optionGroup>
      <optionGroup id="usbCapacities">
        <option id="none">
            <title>None</title>
        </option>
        <option id="sixteen">
            <title>16GB</title>
        </option>
        <option id="thirtytwo">
            <title>32GB</title>
        </option>
      </optionGroup>
      <optionGroup id="usbColor">
        <option id="none">
            <title>None</title>
        </option>
        <option id="usb_silver">
            <title>Silver</title>
        </option>
        <option id="usb_black">
            <title>Black</title>
        </option>
      </optionGroup>

      <!--  天窗形状  -->
      <optionGroup id="cameoShape">
        <option id="none"/>
        <option id="Box_Rect" />
        <option id="Box_Round" />
      </optionGroup>
      <!--  天窗类型  -->
      <optionGroup id="cameo">
        <option id="none"/>
        <option id="Box_M" />
      </optionGroup>

      <optionGroup id="paper">
        <option id="none">
            <title>None</title>
        </option>
        <option id="EP">
            <title>Lustre Print</title>
        </option>
      </optionGroup>

    </options>
  </global>
  <configurableOptionMap>
    <optionMap id="size" keyPattern="product">
      <entry key="IB" value="8X8,10X10,12X12,14X14,8X11,11X14,12X16,11X8,14X11,16X12" defaultValue="8X8"/>
      <entry key="woodBox" value="4X6,5X7,8X10" defaultValue="4X6"/>
      <entry key="usbCase" value="6.5X6.5" defaultValue="6.5X6.5"/>
      <entry key="dvdCase" value="6.5X6.5" defaultValue="6.5X6.5"/>
    </optionMap>

    <optionMap id="type" keyPattern="product-dvdType">
      <entry key="woodBox-none" value="none" defaultValue="none"/>
      <entry key="IB-none" value="BL,IW" defaultValue="BL"/>
      <entry key="usbCase-none" value="BP,IP" defaultValue="BP"/>
      <entry key="dvdCase-one" value="BP,IP" defaultValue="BP"/>
      <entry key="dvdCase-two" value="none" defaultValue="none"/>
    </optionMap>

    <optionMap id="cover" keyPattern="product">
      <entry key="[usbCase,dvdCase]" value="HC,NC,BC,LC" defaultValue="HC"/>
      <entry key="[woodBox,IB]" value="none" defaultValue="none"/>
    </optionMap>

    <optionMap id="leatherColor" keyPattern="cover">
      <entry key="HC" value="none" />
      <entry key="NC" value="black,navyBlue,shamrockGreen,tangerineOrange,slateGrey,maroon,lightBlue,siennaBrown,cream,oliveGreen,indianRed,chocolateBrown,greenLeaf,fireBrick,pinkRose,pansy" />
      <entry key="LC" value="black,red,white,espresso,camel,blue,kiwi,pink,cream,brown,darkGreen,orange,tan,blackNavyBlue,blueGrey,sunnyYellow" />
      <entry key="BC" value="blingLace,blingZebra,blingCream,blingBlack,blingBlue,blingEspresso,blingRed,blingWhite" />
    </optionMap>

    <optionMap id="spineThickness" keyPattern="product">
      <entry key="IB" value="T,L" defaultValue="T"/>
      <entry key="woodBox" value="none"/>
      <entry key="[usbCase,dvdCase]" value="N"/>
    </optionMap>

    <optionMap id="finish" keyPattern="product">
      <entry key="woodBox" value="glossy,matte" defaultValue="matte"/>
      <entry key="[IB,usbCase,dvdCase]" value="none"/>
    </optionMap>

    <optionMap id="dvdType" keyPattern="product">
      <entry key="dvdCase" value="one,two" defaultValue="one"/>
      <entry key="[woodBox,usbCase,IB]" value="none"/>
    </optionMap>

    <optionMap id="dvdPrinted" keyPattern="product">
      <entry key="dvdCase" value="customerPrinted,noPrinted" defaultValue="customerPrinted"/>
      <entry key="[woodBox,usbCase,IB]" value="none"/>
    </optionMap>

    <optionMap id="usbCapacities" keyPattern="product">
      <entry key="usbCase" value="sixteen,thirtytwo" defaultValue="sixteen"/>
      <entry key="[woodBox,dvdCase,IB]" value="none"/>
    </optionMap>

    <optionMap id="usbColor" keyPattern="product">
      <entry key="usbCase" value="usb_silver,usb_black" defaultValue="usb_silver"/>
      <entry key="[woodBox,dvdCase,IB]" value="none"/>
    </optionMap>

    <optionMap id="cameo" keyPattern="cover">
      <entry key="[HC,none]" value="none" />
      <entry key="[LC,NC,BC]" value="Box_M" defaultValue="Box_M" />
    </optionMap>

    <optionMap id="cameoShape" keyPattern="cover">
      <entry key="[HC,none]" value="none" />
      <entry key="[LC,NC,BC]" value="Box_Rect,Box_Round" defaultValue="Box_Rect" />
    </optionMap>

    <optionMap id="paper" keyPattern="product">
      <entry key="woodBox" value="none" />
      <entry key="[usbCase,dvdCase,IB]" value="EP" defaultValue="EP" />
    </optionMap>

  </configurableOptionMap>
