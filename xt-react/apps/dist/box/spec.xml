      <entry key="T" baseValue="827"/>
      <entry key="L" baseValue="579"/>
      <entry key="N" baseValue="130"/>
    </parameter>

    <parameter id="spineExpanding" keyPattern="spineThickness">
      <entry key="[T,L]" expandingOverBackcover="0" expandingOverFrontcover="0" />
      <entry key="N" expandingOverBackcover="53" expandingOverFrontcover="53" />
    </parameter>

    <parameter id="dvdSize" keyPattern="product">
      <entry key="dvdCase" width="1418" height="1418"/>
